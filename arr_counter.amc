.section data:
	dword	min_arr[255]		{#0}
	byte	str	"Array counting example.\n"
	byte	result	"Array accumulate result: "
	byte 	nl	"\n"
	align #3
	byte	alinged	"alinged str\n"

.section code:
	call @_main
	mov %r0, #0
	end

@_main:
	push %bp
	mov %bp, %sp

	pstr str
	
	mov %r0, offset(min_arr)
	mov %r1, lengthof(min_arr)
	mov %r2, #0

.l1:
	cmp %r1, %r2
	load dword [%r0*4], %r4
	add %r2, %r4
	inc %r0
	jmp .l1

.l2
	pstr result
	pint %r2
	pstr nl

	pop %bp
	ret
