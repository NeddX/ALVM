.section data:
	str			ZF_FLAG			"Zero Flag: "
	str			CF_FLAG			"Carry Flag: "
	str			OF_FLAG			"Overflow Flag: "
	str			SF_FLAG			"Sign Flag: "
	str			PF_FLAG			"Parity Flag: "
	str			RESULT			"Result: "	
	str			NL				"\n"	
	const		ZE_KONST		#255
	const		UINT32_MAX		#4294967295

.section code:	
	call @_main	
	mov %r0, #0
	end

@_main:
	push %bp
	mov %bp, %sp

	mov %r0, #100
	mov %r1, #100
	cmp %r0, %r1	

	pstr RESULT
	pint dword [%bp-#4]
	pstr NL
	pstr NL

	pstr ZF_FLAG
	pint %zf
	pstr NL

	pstr CF_FLAG
	pint %cf
	pstr NL

	pstr OF_FLAG
	pint %of
	pstr NL

	pstr SF_FLAG
	pint %sf
	pstr NL

	pstr PF_FLAG
	pint %pf
	pstr NL
	pstr NL
	
	ret

@int_print:
	push %bp
	mov %bp, %sp

	mov %r0, #0
	mov %r1, #255

.l1:
	cmp %r0, %r1	
	jl .l2
	pint %r0
	pstr NL
	inc %r0
	jmp .l1

.l2:
	pop %bp
	ret
